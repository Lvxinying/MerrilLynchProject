	public static String getId(String name) {
		String sql_dataId = "SELECT [DataId] FROM [ReferenceData].[dbo].[DataID_XOI_Map] where XOI_NAME ='%s'";
		Object dataId = null;
		try {
			dataId = DataId.class.getField(name).get(DataId.class);
		} catch (Exception e) {
			dataId = DBCommons.getData(String.format(sql_dataId, name), Database.geoutputdb61);
		}

		if (dataId == null) {
			logger.debug(name + " has no dataId");
			return name;
		}
		return String.valueOf(dataId);
	}

	public static String getIdFromSql(String name, String sql) {
